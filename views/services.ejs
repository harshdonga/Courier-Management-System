<body class="js" onload="loader()">
    <script>
         $(document).ready(function() {
         var x = $("#currentuser").html();
         alert("Hello " + x);
         });
        function loader(){
            Push.create("Hello user!", {
                body: "",
                icon: "/icon.png",
                timeout: 5000,
                onClick: function() {
                    console.log(this);
            }
        });
    }

        function package(){
            var weight = $('#weight').val()
            var origin = $('#origin').val()
            var destination = $('#destination').val()
            var package = {
                weight:weight,
                origin:origin,
                destination:destination
            }

            $.ajax({
                url:"/services",
                type:'POST',
                data: JSON.stringify(package),
                dataType: 'json',
                contentType: 'application/json',
                success: function (res) {
                    if(res.success){
                        alert(res.message)
                    }
                    else{
                        alert(res.message)
                    }
                }
            })
        }
    </script>

